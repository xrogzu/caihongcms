<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.caihong.cms.entity.main">
	<class name="GrainBuyConfig" table="jc_grain_buy_config">
		<meta attribute="sync-DAO">false</meta>
		<cache usage="read-write"/>
		<id name="id" type="java.lang.Integer" column="id"><generator class="identity"/></id>
		<property name="count" column="count" type="integer" not-null="true" length="11"/>
		<property name="price" column="price" type="java.lang.Double" not-null="true" length="20"/>
		<property name="time" column="time" type="timestamp" not-null="false" length="19"/>
	</class>
		
</hibernate-mapping>